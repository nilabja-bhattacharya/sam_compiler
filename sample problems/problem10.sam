int mat1[10][10], mat2[10][10],res[10][10];
void multiply()
{ 
    for (int i = 0; i < 10; i++) 
    { 
        for (int j = 0; j < 10; j++) 
        { 
            res[i][j] = 0; 
            for (int k = 0; k < 10; k++) 
                res[i][j] += mat1[i][k] *  
                             mat2[k][j]; 
        } 
    } 
} 
  